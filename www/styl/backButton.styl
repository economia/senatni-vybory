&
    position absolute
    top 15px
    left 45px
    display block
    width 38px
    height 38px
    border 2px solid #1d1d1d
    transition left 1s
    border-radius 50%
    background white url('../img/back.png') -2px -2px
    &:hover
        background-color #eee
        cursor pointer
    &:active
        background-color #aaa
    img
        position absolute
        top -2px
        left -2px
    &.disabled
        left -80px
        // display none
